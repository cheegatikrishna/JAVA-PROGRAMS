import java.util.*;
public class Reverse
{  
public static void main(String[] args)   
{  
    Scanner sc=new Scanner(System.in);
    {
        int num,reverse=0;  
        num=sc.nextInt();
        while(num!=0)
        {  
            int remainder =num % 10;  
            reverse = reverse * 10 + remainder;  
            num=num/10;  
            
        }  
        System.out.println(reverse);  
        }
    } 
}
