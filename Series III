import java.util.*;
public class Series3
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int n;
        int a=3;
        n=sc.nextInt();
        System.out.printf("\t"+a);
        for(int i=1;i<=n;i++)
        {  
            a=a*3;
            System.out.printf("\t"+a);
        }
    }
}
