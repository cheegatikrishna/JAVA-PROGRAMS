import java.util.*;
public class Three
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a,b,c,d;
        float e,f;
        a=sc.nextInt();
        b=sc.nextInt();
        c=sc.nextInt();
        d=sc.nextInt();
        e=a+c;
        f=b+d;
        System.out.printf("Binoy's house is located at (%.1f,%.1f)",e/2,f/2);
    }
}
