import java.util.*;
public class Series8
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int n;
        int a=4;
        int b=1;
        n=sc.nextInt();
         System.out.print(a+" ");
        for(int i=2;i<=n;i++)
        {
            a=a+b;
            System.out.print(a+" ");
            b=i*i;
            
        }
    }
}
