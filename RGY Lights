import java.util.*;
public class Lights
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        float a,b,c,d;
        a=sc.nextFloat();
        b=sc.nextFloat();
        c=sc.nextFloat();
        d=a/b;
        if(d>c)
        {
            System.out.println("yes");
        }
        else
        {
            System.out.println("no");
        }
        
        //Write your code
    }
}
