import java.util.*;
public class Perfect
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int i=sc.nextInt();
        if(i%2==0)
        {
                System.out.println("Perfect number");
            }
            else
            {
                System.out.println(" Not a Perfect number");
            }
    }
}
