import java.util.*;
public class Avg
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        float a,b,c,d,e,avg;
        a=sc.nextFloat();
        b=sc.nextFloat();
        c=sc.nextFloat();
        d=sc.nextFloat();
        e=sc.nextFloat();
        avg=(a+b+c+d+e)/5;
        System.out.printf("%.2f",avg);
        
        //write your code
    }
}
