import java.util.Scanner;
public class Factorial
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int n,fact=6;
        n=sc.nextInt();
        if(n==fact)
        {
            System.out.println("yes");
        }
        else
        {
             System.out.println("No");
        }
    }
}
