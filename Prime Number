import java.util.*;
public class Prime
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        {
        if(n%2==1 || n==2)
        {
            System.out.println("Prime number");
        }
        else
        {
            System.out.println("Not a Prime");
        }
        }
    }
}
