import java.util.*;
public class Swap
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a,b,c;
        a=sc.nextInt();
        b=sc.nextInt();
        c=a+b;
        a=c-a;
        b=c-a;
        System.out.println(a);
        System.out.println(b);
    }
}
