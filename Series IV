import java.util.*;
public class Series4
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        double a=0.5;
        int b=1;
        int n=sc.nextInt();
        System.out.printf("\t"+a);
        for(int i=1;i<=n;i++)
        {
            a=a+b;
            b=b*3;
            System.out.printf("\t"+a);
        }
        
        //Write your code
    }
}
