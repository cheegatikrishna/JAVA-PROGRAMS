import java.util.*;
public class Series1
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int n,sum;
        n=sc.nextInt();
        for(int i=1;i<=n;i++)
        {
            sum=i*i;
            System.out.printf("\t"+sum);
        }
    }
    
}
