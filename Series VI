import java.util.*;
public class Series6
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a,n;
        n=sc.nextInt();
        for(int i=1;i<=n;i++)
        {
            if(i%2==0)
            {
                a=i*i-2;
                System.out.print(a+"\t");
            }
            else
            {
                a=(i*i)-1;
                System.out.print(a+"\t");
            }
        }
    }
}
