import java.util.*;
public class Lcm
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a,b;
        int c=0,e=0;
        a=sc.nextInt();
        b=sc.nextInt();
        for(int i=1;i<=a;i++)
        {
            if(a%i==0 && b%i==0)
            {
                c=i;
                e=(a*b)/c;
            }
        }
            System.out.printf("LCM of %d and %d is %d",a,b,e);
    }
}
