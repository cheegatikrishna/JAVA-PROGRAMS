import java.util.*;
import java.util.Arrays;
public class Sort
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++)
        {
            a[i]=sc.nextInt();
        }
        for(int i=0;i<n;i++)
        {
            Arrays.sort(a);
            System.out.println(a[i]);
        }
    }
}
