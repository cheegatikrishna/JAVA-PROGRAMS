import java.util.*;
public class Series5
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=11;
        int n=sc.nextInt();
        for(int i=0;i<n;i++)
        {
            System.out.printf("\t"+(a*a));
            a=a+4;
        }
    }
}
