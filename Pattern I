import java.util.*;
public class Pattern1
{
    public static void main(String[] args)
    {
            Scanner sc=new Scanner(System.in);
        int num;
        num=sc.nextInt();
        int i=1;
        for(i=1;i<=num;i++)
        {
           int j =1;
            for(j=1;j<=num;j++)
            {
                System.out.print("");
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
