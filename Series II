import java.util.*;
public class Series2
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int a=6,b=5;
        int n=sc.nextInt();
        System.out.printf("\t",+a);
        for(int i=1;i<=n;i++)
        {
            System.out.print("\t"+a);
            a=a+b;
            b=b+5;
        }
    }
}
