import java.util.*;
public class Sum
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);//Scanner is a class
        int n,sum=0,digit;//variable declaration and initilation
        n=sc.nextInt();
        while(n>0)
        {
            digit=n%10;
            sum=sum+digit;
            n=n/10;
        }
        System.out.println(sum);
    }
}
    
